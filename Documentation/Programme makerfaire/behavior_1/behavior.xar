                                                        <![CDATA[import json
import urllib2

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        self.tts = ALProxy('ALTextToSpeech')
    def onLoad(self):
        #put initialization code here
        pass

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self):
        #self.onStopped() #activate the output of the box
        #self.tts.say("Hello young Padawan")
        f = urllib2.urlopen("http://api.openweathermap.org/data/2.5/forecast?q=Perpignan,fr&mode=json&apikey=4f4483fabe2c5ebf8b5b9f13d0a60c0b&lang=fr&units=metric")
        self.logger.debug( "1 " )
        raw = f.read()
        #self.logger.debug( "2 "+raw )
        data = json.loads(raw)
        #self.logger.info(data)
        #0 = maintenant
        #9 = demain
        #18 = après-demain
        temps = data['list'][0]['weather'][0]['description'].encode('utf-8').strip()
        #temps = data['main']
        #self.logger.info('il faira '+ str(temps) )
        #self.tts.say('Le temps est '+ str(temps)
        self.tts.say('Le temps est '+ str((int(temps) ))
        self.onStopped() # activate
        pass

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]>
</content>
